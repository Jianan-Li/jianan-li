howModal: false,\n      currentModalItem: \"\"\n    };\n  },\n  created: function created() {\n    window.addEventListener(\"keydown\", this.handleKeydown);\n  },\n  destroyed: function destroyed() {\n    window.removeEventListener(\"keydown\", this.handleKeydown);\n  },\n  methods: {\n    handleKeydown: function handleKeydown(e) {\n      if (e.key == \"Escape\") {\n        this.modalDisplayMode = \"none\";\n        this.closeModal();\n      }\n    },\n    openModal: function openModal(item) {\n      this.currentModalItem = item;\n      this.showModal = true;\n      this.disableBodyScroll();\n    },\n    disableBodyScroll: function disableBodyScroll() {\n      var targetElement = document.getElementById(\"empty-div\"); // console.log(targetElement);\n\n      _disableBodyScroll(targetElement);\n    },\n    closeModal: function closeModal() {\n      this.showModal = false;\n      clearAllBodyScrollLocks();\n    }\n  }\n};",{"version":3,"sources":["Gallery.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2EA,SAAA,iBAAA,IAAA,kBAAA,EAAA,uBAAA,QAAA,kBAAA;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,SADA;AAEA,EAAA,KAAA,EAAA;AACA,IAAA,IAAA,EAAA,MADA;AAEA,IAAA,KAAA,EAAA;AAFA,GAFA;AAMA,EAAA,IAAA,EAAA;AAAA,WAAA;AACA,MAAA,SAAA,EAAA,KADA;AAEA,MAAA,gBAAA,EAAA;AAFA,KAAA;AAAA,GANA;AAUA,EAAA,OAVA,qBAUA;AACA,IAAA,MAAA,CAAA,gBAAA,CAAA,SAAA,EAAA,KAAA,aAAA;AACA,GAZA;AAaA,EAAA,SAbA,uBAaA;AACA,IAAA,MAAA,CAAA,mBAAA,CAAA,SAAA,EAAA,KAAA,aAAA;AACA,GAfA;AAgBA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,yBACA,CADA,EACA;AACA,UAAA,CAAA,CAAA,GAAA,IAAA,QAAA,EAAA;AACA,aAAA,gBAAA,GAAA,MAAA;AACA,aAAA,UAAA;AACA;AACA,KANA;AAOA,IAAA,SAPA,qBAOA,IAPA,EAOA;AACA,WAAA,gBAAA,GAAA,IAAA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,iBAAA;AACA,KAXA;AAYA,IAAA,iBAZA,+BAYA;AACA,UAAA,aAAA,GAAA,QAAA,CAAA,cAAA,CAAA,WAAA,CAAA,CADA,CAEA;;AACA,MAAA,kBAAA,CAAA,aAAA,CAAA;AACA,KAhBA;AAiBA,IAAA,UAjBA,wBAiBA;AACA,WAAA,SAAA,GAAA,KAAA;AACA,MAAA,uBAAA;AACA;AApBA;AAhBA,CAAA","sourcesContent":["<template>\n  <div class=\"gallery\">\n    <div class=\"flex-container\">\n      <div\n        class=\"gallery-item card\"\n        @click=\"openModal(item)\"\n        v-for=\"item in media\"\n        :key=\"item\"\n      >\n        <picture v-if=\"item.includes('.jpg')\">\n          <source\n            class=\"model-picture-webp\"\n            type=\"image/webp\"\n            :srcset=\"require(`@/assets/${path}/${item.slice(0, -3)}webp`)\"\n          />\n          <img :src=\"require(`@/assets/${path}/${item}`)\" :alt=\"item\" />\n        </picture>\n        <!-- <img\n          v-if=\"item.includes('.jpg')\"\n          :src=\"require(`@/assets/${path}/${item}`)\"\n          :alt=\"item\"\n        /> -->\n        <video v-if=\"item.includes('.mp4')\" autoplay loop muted playsinline>\n          <source :src=\"require(`@/assets/${path}/${item}`)\" type=\"video/mp4\" />\n        </video>\n      </div>\n    </div>\n\n    <!-- An empty div for use with disableBodyScroll() -->\n    <div id=\"empty-div\"></div>\n\n    <!-- The Modal -->\n    <transition name=\"modal\">\n      <div class=\"modal\" @click.self=\"closeModal()\" v-if=\"showModal\" id=\"modal\">\n        <div v-if=\"currentModalItem.includes('.jpg')\" class=\"modal-content\">\n          <picture @click.self=\"closeModal()\" class=\"model-content-picture\">\n            <source\n              type=\"image/webp\"\n              :srcset=\"\n                require(`@/assets/${path}/${currentModalItem.slice(0, -3)}webp`)\n              \"\n            />\n            <img\n              class=\"card-modal\"\n              :src=\"require(`@/assets/${path}/${currentModalItem}`)\"\n              :alt=\"currentModalItem\"\n            />\n          </picture>\n          <!-- <img\n            :src=\"require(`@/assets/${path}/${currentModalItem}`)\"\n            :alt=\"currentModalItem\"\n          /> -->\n        </div>\n        <div v-if=\"currentModalItem.includes('.mp4')\" class=\"modal-content\">\n          <video\n            @click.self=\"closeModal()\"\n            class=\"model-content-video\"\n            autoplay\n            loop\n            muted\n            playsinline\n          >\n            <source\n              class=\"video-source\"\n              :src=\"require(`@/assets/${path}/${currentModalItem}`)\"\n              type=\"video/mp4\"\n            />\n          </video>\n        </div>\n      </div>\n    </transition>\n  </div>\n</template>\n\n<script>\nimport { disableBodyScroll, clearAllBodyScrollLocks } from \"body-scroll-lock\";\n\nexport default {\n  name: \"Gallery\",\n  props: {\n    path: String,\n    media: Array\n  },\n  data: () => ({\n    showModal: false,\n    currentModalItem: \"\"\n  }),\n  created() {\n    window.addEventListener(\"keydown\", this.handleKeydown);\n  },\n  destroyed() {\n    window.removeEventListener(\"keydown\", this.handleKeydown);\n  },\n  methods: {\n    handleKeydown(e) {\n      if (e.key == \"Escape\") {\n        this.modalDisplayMode = \"none\";\n        this.closeModal();\n      }\n    },\n    openModal(item) {\n      this.currentModalItem = item;\n      this.showModal = true;\n      this.disableBodyScroll();\n    },\n    disableBodyScroll() {\n      const targetElement = document.getElementById(\"empty-div\");\n      // console.log(targetElement);\n      disableBodyScroll(targetElement);\n    },\n    closeModal() {\n      this.showModal = false;\n      clearAllBodyScrollLocks();\n    }\n  }\n};\n</script>\n\n<style scoped>\n.flex-container {\n  margin: 0rem -0.5rem -0.5rem;\n  padding-top: 2.5rem;\n}\n.gallery-item {\n  flex: 1 0 34%; /* explanation below */\n  margin: 0.5rem;\n  cursor: pointer;\n}\n@media screen and (max-width: 600px) {\n  .flex-container {\n    padding-top: 0.5rem;\n  }\n}\nvideo {\n  display: flex;\n  max-width: 100%;\n  max-height: 100%;\n}\n.modal {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  position: fixed;\n  width: 100%;\n  height: 100%;\n}\n.model-content-picture {\n  display: flex;\n  width: 100%;\n  height: 100%;\n}\n.modal-content {\n  display: flex;\n  height: 90%;\n  width: auto;\n  max-height: 90%;\n  max-width: 90%;\n  /* cursor: auto; */\n}\nimg,\n.video-source {\n  cursor: auto;\n}\n/* .close {\n  position: absolute;\n  top: 13px;\n  right: 15px;\n  transition: 0.3s;\n  cursor: pointer;\n  font-size: 28px;\n  color: #999999;\n}\n.close:hover {\n  color: #4a4a4a;\n} */\n</style>\n"],"sourceRoot":"src/components"}]}